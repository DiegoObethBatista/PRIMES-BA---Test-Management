{
  "name": "@primes-ba/backend",
  "version": "1.0.0",
  "description": "Backend API for PRIMES BA Test Management",
  "main": "dist/index.js",
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "build": "tsc",
    "start": "node dist/index.js",
    "clean": "rm -rf dist",
    "typecheck": "tsc --noEmit",
    "lint": "eslint src --ext .ts",
